<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.shape.Circle?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         style="-fx-background-color: #001f3f; -fx-background-radius: 35; -fx-border-radius: 35; -fx-border-color: #7FDBFF;"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
    <Label fx:id="dialog" text="Label" style="-fx-text-fill: #FFFAF1" wrapText="true">
        <padding>
            <Insets left="15.0" right="15.0"/>
        </padding>
        <font>
            <Font size="13.0"/>
        </font>
    </Label>
<!--    <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true"/>-->
    <Circle fx:id="circle" radius="60" stroke="seagreen" pickOnBounds="true"/>
</fx:root>